            </complication>
        </data>
        <dataColors>
            <color label="@Strings.colorRed">0xFF0000</color>
            <color label="@Strings.colorWhite" default="true">0xFFFFFF</color>
            <color label="@Strings.colorLightGray">0xC0C0C0</color>
        </dataColors>
    </watchface-config>
</resources>