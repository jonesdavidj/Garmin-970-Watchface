        <styles>
            <style id="1" label="@Strings.style1"/>
            <style id="2" label="@Strings.style2"/>
            <style id="3" label="@Strings.style3" default="true"/>
        </styles>
        <accentColors allowAny="true"/>
        <data>
            <complication id="1" allowAny="true"/>
